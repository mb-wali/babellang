{
	"name": "babellang",
	"displayName": "BabelLang",
	"publisher": "mbwali",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/mb-wali/babellang.git"
	  },
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:babellang.trans1",
		"onCommand:babellang.trans2"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "babellang.trans1",
				"title": "_ Translation"
			},
			{
				"command": "babellang.trans2",
				"title": "Tag Translation"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "babellang.trans1",
					"group": "1_modification"
				},
				{
					"command": "babellang.trans2",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
